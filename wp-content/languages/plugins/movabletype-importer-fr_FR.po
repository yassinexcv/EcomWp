msgid "Movable Type and TypePad"
msgstr "Movable Type et TypePad"

#: movabletype-importer.php:481
msgid "All done. <a href=\"%s\">Have fun!</a>"
msgstr "Terminé&nbsp;! (<a href=\"%s\">Amusez-vous bien&nbsp;!</a>)"

